DESELECTS=()
if [ $quoting = rules ]
then
    for test in "${EXCLUDES[@]}"
    do
        DESELECTS+=("--deselect='$test'")
    done
    echo ${DESELECTS[*]}
else
    for test in "${EXCLUDES[@]}"
    do
        DESELECTS+=("--deselect=$test")
    done
    echo "${DESELECTS[*]}"
fi
